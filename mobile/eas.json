{
  "cli": {
    "version": ">= 13.0.0",
    "appVersionSource": "local",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "base": {
      "node": "20.18.0",
      "env": {
        "EXPO_PUBLIC_WEB_APP_URL": "https://dealgapiq.com"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "http://localhost:8000",
        "APP_ENV": "development"
      }
    },
    "development-device": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://dealscope-production.up.railway.app",
        "APP_ENV": "development"
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://dealscope-production.up.railway.app",
        "APP_ENV": "preview"
      }
    },
    "beta": {
      "extends": "base",
      "distribution": "internal",
      "channel": "beta",
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://dealscope-production.up.railway.app",
        "APP_ENV": "beta"
      }
    },
    "production": {
      "extends": "base",
      "distribution": "store",
      "channel": "production",
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium",
        "credentialsSource": "remote"
      },
      "android": {
        "buildType": "app-bundle",
        "resourceClass": "medium",
        "credentialsSource": "remote"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://dealscope-production.up.railway.app",
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "dev@dealgapiq.com",
        "ascAppId": "6759636866",
        "appleTeamId": "A2Y6C3NNSY",
        "sku": "com.dealgapiq.mobile",
        "language": "en-US"
      },
      "android": {
        "serviceAccountKeyPath": "./google-services-key.json",
        "track": "production",
        "releaseStatus": "completed",
        "changesNotSentForReview": false
      }
    },
    "beta": {
      "ios": {
        "appleId": "dev@dealgapiq.com",
        "ascAppId": "6759636866",
        "appleTeamId": "A2Y6C3NNSY",
        "sku": "com.dealgapiq.mobile",
        "language": "en-US"
      },
      "android": {
        "serviceAccountKeyPath": "./google-services-key.json",
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
