{
  "expo": {
    "name": "DealGapIQ",
    "slug": "dealgapiq",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dealgapiq",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "413e1c97-3bfc-44ef-9437-e8a89b79cde8"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dealgapiq.mobile",
      "buildNumber": "1",
      "associatedDomains": [
        "applinks:dealgapiq.com",
        "applinks:www.dealgapiq.com"
      ],
      "infoPlist": {
        "NSCameraUsageDescription": "DealGapIQ uses your camera to scan property addresses for instant analysis.",
        "NSLocationWhenInUseUsageDescription": "DealGapIQ uses your location to find nearby properties for analysis."
      }
    },
    "android": {
      "package": "com.dealgapiq.mobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "dealgapiq.com",
              "pathPrefix": "/property"
            },
            {
              "scheme": "https",
              "host": "dealgapiq.com",
              "pathPrefix": "/verdict"
            },
            {
              "scheme": "https",
              "host": "dealgapiq.com",
              "pathPrefix": "/verify-email"
            },
            {
              "scheme": "https",
              "host": "dealgapiq.com",
              "pathPrefix": "/reset-password"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-localization",
      "@sentry/react-native"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
