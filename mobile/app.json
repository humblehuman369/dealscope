{
  "expo": {
    "name": "DealGapIQ",
    "slug": "dealgapiq",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dealgapiq",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "a75cf449-712d-4ac2-9463-30fca751c69c"
      },
      "router": {}
    },
    "owner": "dealgapiq",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dealgapiq.mobile",
      "buildNumber": "1",
      "associatedDomains": [
        "applinks:dealgapiq.com",
        "applinks:www.dealgapiq.com"
      ],
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription": "DealGapIQ uses your camera to scan property addresses from for-sale signs and listings for instant investment analysis.",
        "NSLocationWhenInUseUsageDescription": "DealGapIQ uses your location to find nearby properties for investment analysis.",
        "NSFaceIDUsageDescription": "Use Face ID to quickly unlock DealGapIQ and protect your investment data.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
            "NSPrivacyAccessedAPITypeReasons": [
              "C617.1"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.dealgapiq.mobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "dealgapiq.com",
              "pathPrefix": "/property"
            },
            {
              "scheme": "https",
              "host": "dealgapiq.com",
              "pathPrefix": "/verdict"
            },
            {
              "scheme": "https",
              "host": "dealgapiq.com",
              "pathPrefix": "/verify-email"
            },
            {
              "scheme": "https",
              "host": "dealgapiq.com",
              "pathPrefix": "/reset-password"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization",
      [
        "expo-camera",
        {
          "cameraPermission": "DealGapIQ uses your camera to scan property addresses from for-sale signs and listings."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/android-icon-monochrome.png",
          "color": "#0EA5E9"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "dealgapiq",
          "project": "mobile"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
